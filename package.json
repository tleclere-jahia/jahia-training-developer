  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "react-scripts": "^5.0.1",
    "react-table": "^7.8.0",
    "rimraf": "^5.0.1",
