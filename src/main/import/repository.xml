<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <jahia-training-developer j:dependencies="default external-provider-users-groups graphql-dxm-provider jahia-authentication jahia-oauth-aws-cognito location rating server-availability-manager"
                                j:modulePriority="0"
                                j:moduleType="module"
                                j:title="jahia-training-developer"
                                jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder">
            <training jcr:primaryType="jnt:contentFolder">
               <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Training"/>
               <jahia-solutions-group j:geocodeAutomatically="true"
                                      jcr:mixinTypes="jmix:geotagged"
                                      jcr:primaryType="foont:company">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Jahia Solutions Group"/>
                  <tlc firstname="Thomas"
                       j:geocodeAutomatically="true"
                       jcr:mixinTypes="jmix:geotagged"
                       jcr:primaryType="foont:employee"
                       jcr:title="TLC"
                       lastname="LE CLERE">
                     <j:translation_en biography="&lt;p&gt;&lt;strong&gt;Lorem Ipsum&lt;/strong&gt;&amp;nbsp;is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry&amp;#39;s standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries, but also the leap into electronic typesetting, remaining essentially unchanged. It was popularised in the 1960s with the release of Letraset sheets containing Lorem Ipsum passages, and more recently with desktop publishing software like Aldus PageMaker including versions of Lorem Ipsum.&lt;/p&gt;&#xA;"
                                       jcr:language="en"
                                       jcr:primaryType="jnt:translation"/>
                  </tlc>
               </jahia-solutions-group>
               <trainees j:geocodeAutomatically="true"
                         jcr:mixinTypes="jmix:geotagged"
                         jcr:primaryType="foont:company">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Trainees"/>
               </trainees>
            </training>
         </contents>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <employee-content-template j:applyOn="foont:employee"
                                       j:defaultTemplate="true"
                                       j:hiddenTemplate="false"
                                       jcr:primaryType="jnt:contentTemplate">
               <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Employee content template"/>
               <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                            jcr:primaryType="jnt:contentList">
                  <main-resource-display j:mainResourceView="details" jcr:primaryType="jnt:mainResourceDisplay"/>
                  <map j:height="300"
                       j:mapType="roadmap"
                       j:width="500"
                       j:zoom="7"
                       jcr:primaryType="jnt:map"/>
                  <back jcr:primaryType="jnt:backToParentPage">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                  </back>
                  <rating jcr:primaryType="jnt:rateable"/>
                  <display-comments jcr:primaryType="jnt:commentsList"/>
                  <commentable jcr:primaryType="jnt:addComment"/>
               </pagecontent>
            </employee-content-template>
         </templates>
      </jahia-training-developer>
   </modules>
</content>
