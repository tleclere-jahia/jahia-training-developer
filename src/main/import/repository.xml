<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <jahia-training-developer j:dependencies="default location"
                                j:modulePriority="0"
                                j:moduleType="module"
                                j:title="jahia-training-developer"
                                jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                jcr:primaryType="jnt:module">
         <sources jcr:mixinTypes="jmix:hasExternalProviderExtension"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-jahia-training-developer-1.0.0-SNAPSHOT">
            <src jcr:mixinTypes="jmix:hasExternalProviderExtension"
                 jcr:primaryType="jnt:folder">
               <main jcr:mixinTypes="jmix:hasExternalProviderExtension"
                     jcr:primaryType="jnt:folder">
                  <resources jcr:mixinTypes="jmix:hasExternalProviderExtension"
                             jcr:primaryType="jnt:folder">
                     <foont_latestEmployees jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                            jcr:primaryType="jnt:nodeTypeFolder">
                        <html jcr:mixinTypes="jmix:hasExternalProviderExtension"
                              jcr:primaryType="jnt:templateTypeFolder">
                           <latestEmployees.hidden.load.jsp jcr:primaryType="jnt:viewFile"/>
                        </html>
                     </foont_latestEmployees>
                  </resources>
               </main>
            </src>
         </sources>
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <employee-content-template j:applyOn="foont:employee"
                                       j:defaultTemplate="true"
                                       j:hiddenTemplate="false"
                                       jcr:primaryType="jnt:contentTemplate">
               <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Employee content template"/>
               <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                            jcr:primaryType="jnt:contentList">
                  <main-resource-display j:mainResourceView="details" jcr:primaryType="jnt:mainResourceDisplay"/>
                  <map j:height="300"
                       j:mapType="roadmap"
                       j:width="500"
                       j:zoom="7"
                       jcr:primaryType="jnt:map"/>
                  <back jcr:primaryType="jnt:backToParentPage">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                  </back>
               </pagecontent>
            </employee-content-template>
            <home j:hiddenTemplate="false"
                  j:view="default"
                  jcr:primaryType="jnt:pageTemplate">
               <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Home"/>
               <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                            jcr:primaryType="jnt:contentList">
                  <image-header j:allowedTypes="jnt:imageI18nReference"
                                j:level="0"
                                j:limitedAbsoluteAreaEdit="false"
                                jcr:primaryType="jnt:absoluteArea"/>
                  <pagecontent j:allowedTypes="jmix:structuredContent" jcr:primaryType="jnt:area"/>
               </pagecontent>
            </home>
         </templates>
      </jahia-training-developer>
   </modules>
</content>